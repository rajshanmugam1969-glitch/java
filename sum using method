import java.io.*;
import java.util.*;

public class Solution {
    int add(int a,int b){
        int c=a+b;
        return c;
    }

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        Solution sn=new Solution();
            int a=s.nextInt();
            int b=s.nextInt();
            int x=sn.add(a,b);
        System.out.println(x);
        
    }
}
